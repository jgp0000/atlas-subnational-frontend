<div class="new__buildermod__questions position-relative">

  <div class="container-fluid px-md-x10 pb-5">
    <div class="row row-without-margin">
      <div class="col-sm-12 pl-md-0">
        <h1 class="question__title my-5">Realice una b√∫squeda por:</h1>
      </div>
    </div>
    <div class="row row-without-margin">

      <div class="col-sm-12 col-md-6 pl-md-0">
        <h2 class="new__builder__questions__header">{{t 'graph_builder.builder_questions.land_sown'}}</h2>

        <div class="mt-4">
          <i class="fa fa-arrow-right" aria-hidden="true"></i>
          {{#link-to 'agproduct.visualization' model.id 'departments' 'geo' 'land_sown,production_tons,land_harvested' (query-params startDate=lastYear endDate=lastYear search='null')
            class="builder__questions__question"
          }}
            {{t 'graph_builder.page_title.agproduct.departments.land_sown'}}
          {{/link-to}}
        </div>

        <div class="mt-3">
          <i class="fa fa-arrow-right" aria-hidden="true"></i>
          {{#link-to 'agproduct.visualization' model.id  'municipalities' 'multiples' 'land_sown' (query-params startDate=firstYear endDate=lastYear search='null')
            class="builder__questions__question"
          }}
            {{t 'graph_builder.page_title.agproduct.municipalities.land_sown'}}
          {{/link-to}}
        </div>

      </div>

      <div class="col-sm-12 col-md-6 pt-5 pt-md-0">
        <h2 class="new__builder__questions__header">{{t 'graph_builder.builder_questions.land_harvested'}}</h2>

        <div class="mt-4">
          <i class="fa fa-arrow-right" aria-hidden="true"></i>
          {{#link-to 'agproduct.visualization' model.id 'departments' 'geo' 'land_harvested' (query-params startDate=lastYear endDate=lastYear search='null')
